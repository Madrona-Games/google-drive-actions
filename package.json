{
  "name": "google-drive-actions",
  "description": "Google Drive API actions",
  "version": "1.0.0",
  "author": "Madrona Games Inc",
  "homepage": "https://github.com/Madrona-Games/google-drive-actions",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Madrona-Games/google-drive-actions.git"
  },
  "bugs": {
    "url": "https://github.com/Madrona-Games/google-drive-actions/issues"
  },
  "keywords": [
    "actions",
    "google"
  ],
  "engines": {
    "node": ">=20.x"
  },
  "scripts": {
    "prepare": "lefthook install",
    "build": "npm install && tsc && concurrently \"ncc build --source-map --license licenses.txt -o dist/download src/download/download.ts\" \"ncc build --source-map --license licenses.txt -o dist/create-folder src/create-folder/create-folder.ts\" \"ncc build --source-map --license licenses.txt -o dist/delete src/delete/delete.ts\" \"ncc build --source-map --license licenses.txt -o dist/find src/find/find.ts\" \"ncc build --source-map --license licenses.txt -o dist/upload src/upload/upload.ts\"",
    "test": "jest",
    "lint": "prettier --check \"src/**/*.{js,ts}\" && eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.{js,ts}\""
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "googleapis": "^131.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vercel/ncc": "^0.38.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-jsonc": "^2.12.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^50.0.1",
    "jest": "^29.7.0",
    "jest-fail-on-console": "^3.1.2",
    "lefthook": "^1.6.1",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "volta": {
    "node": "20.11.0"
  }
}
